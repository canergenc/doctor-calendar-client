    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/googlemaps": "^3.39.1",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.9.17",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.5.3",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
