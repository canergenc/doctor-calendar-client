{
  "name": "omnicalendar",
  "version": "1.0.0",
  "description": "omnicalendar",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs",
    "dashboard",
    "dashboard-react"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/pickers": "^3.2.10",
    "@types/moment-range": "^4.0.0",
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "email-validator": "^2.0.4",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "moment": "^2.25.1",
    "moment-range": "^4.0.2",
    "node-sass": "^4.14.0",
    "nouislider": "^13.1.5",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^2.0.0-rc.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-chartjs-2": "^2.8.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-date-picker": "^8.0.1",
    "react-datepicker": "^2.14.1",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.12.0",
    "react-horizontal-scrolling-menu": "^0.7.6",
    "react-material-toast": "^16.1.7",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
